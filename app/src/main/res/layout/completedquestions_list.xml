<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    xmlns:android.support.v7.cardview="http://schemas.android.com/tools"
    app:cardElevation="2sp"
    app:cardUseCompatPadding="true"
    app:cardCornerRadius="5sp"
    android:background="#FFFFFF"
    app:cardBackgroundColor="#FFFFFF"
    android.support.v7.cardview:cardBackgroundColor="@color/white">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/white"
        android:orientation="vertical"
        android:id="@+id/completedLinearLayout">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
    <TextView
        android:id="@+id/cqno"
        android:layout_marginLeft="5px"
        android:layout_marginRight="20px"
        android:layout_marginTop="5px"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textColor="#FFFFFF" />
    <TextView
        android:id="@+id/cqnumber"
        android:layout_marginLeft="5px"
        android:layout_marginRight="20px"
        android:layout_marginTop="5px"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textColor="#000000" />
    </LinearLayout>
    <TextView
        android:id="@+id/cqname"
        android:layout_marginLeft="5px"
        android:layout_marginRight="20px"
        android:layout_marginTop="5px"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textColor="#000000"/>
    <!--<ListView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/optionList"
        android:layout_marginLeft="5px"
        android:layout_marginRight="20px"
        android:layout_marginTop="5px"
        />-->
    </LinearLayout>
</android.support.v7.widget.CardView>